[
  {
    "id": "customer-feedback-form",
    "name": "Customer Feedback Form",
    "description": "Collect feedback from customers after a visit or purchase.",
    "layout": "two-column",
    "createdAt": "2025-01-01T00:00:00.000Z",
    "updatedAt": "2025-01-01T00:00:00.000Z",
    "fields": [
      {
        "id": "cff-full-name",
        "name": "fullName",
        "label": "Full Name",
        "type": "text",
        "required": true,
        "placeholder": "Jane Doe"
      },
      {
        "id": "cff-email",
        "name": "email",
        "label": "Email",
        "type": "text",
        "required": true,
        "placeholder": "jane@example.com",
        "pattern": "^\\S+@\\S+\\.\\S+$"
      },
      {
        "id": "cff-visit-date",
        "name": "visitDate",
        "label": "Date of Visit",
        "type": "date",
        "required": false
      },
      {
        "id": "cff-rating",
        "name": "rating",
        "label": "Overall Rating",
        "type": "select",
        "required": true,
        "options": ["1 - Very Poor", "2 - Poor", "3 - OK", "4 - Good", "5 - Excellent"]
      },
      {
        "id": "cff-would-recommend",
        "name": "wouldRecommend",
        "label": "Would you recommend us?",
        "type": "select",
        "required": true,
        "options": ["Yes", "No", "Not sure"]
      },
      {
        "id": "cff-comments",
        "name": "comments",
        "label": "Additional Comments",
        "type": "text",
        "required": false,
        "placeholder": "Tell us more about your experience"
      }
    ]
  },
  {
    "id": "job-application-form",
    "name": "Job Application Form",
    "description": "Basic job application form for candidates.",
    "layout": "single-column",
    "createdAt": "2025-01-02T00:00:00.000Z",
    "updatedAt": "2025-01-02T00:00:00.000Z",
    "fields": [
      {
        "id": "ja-first-name",
        "name": "firstName",
        "label": "First Name",
        "type": "text",
        "required": true,
        "placeholder": "John"
      },
      {
        "id": "ja-last-name",
        "name": "lastName",
        "label": "Last Name",
        "type": "text",
        "required": true,
        "placeholder": "Smith"
      },
      {
        "id": "ja-email",
        "name": "email",
        "label": "Email",
        "type": "text",
        "required": true,
        "placeholder": "john.smith@example.com",
        "pattern": "^\\S+@\\S+\\.\\S+$"
      },
      {
        "id": "ja-age",
        "name": "age",
        "label": "Age",
        "type": "number",
        "required": true,
        "min": 18,
        "max": 65,
        "placeholder": "25"
      },
      {
        "id": "ja-start-date",
        "name": "startDate",
        "label": "Earliest Start Date",
        "type": "date",
        "required": false
      },
      {
        "id": "ja-department",
        "name": "department",
        "label": "Preferred Department",
        "type": "select",
        "required": true,
        "options": ["Engineering", "Design", "Marketing", "Sales", "Operations"]
      },
      {
        "id": "ja-notes",
        "name": "notes",
        "label": "Notes / Motivation",
        "type": "text",
        "required": false,
        "placeholder": "Short motivation or extra notes"
      }
    ]
  }
]